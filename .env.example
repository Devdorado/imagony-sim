# .env.example - Copy to .env and fill in your values
# NEVER commit .env to git!

# === WALLET ADDRESSES (REQUIRED) ===
# Generate these securely - never share private keys!
LLORD_WALLET=0xFILL_THIS_IN_WITH_YOUR_ADDRESS
WILSOND_WALLET=0xFILL_THIS_IN_WITH_AGENT_PROXY
EMERGENCY_WALLET=0xFILL_THIS_IN_WITH_TRUSTED_3RD_PARTY

# Optional: Can be same as deployer initially
TREASURY_WALLET=0x
DEV_WALLET=0x
LIQUIDITY_WALLET=0x

# === PRIVATE KEYS (KEEP SECRET!) ===
# For deployment only - remove after deployment
DEPLOYER_PRIVATE_KEY=your_private_key_here

# === NETWORK CONFIG ===
# Base Mainnet
BASE_RPC_URL=https://mainnet.base.org
BASE_API_KEY=your_basescan_api_key

# Base Sepolia (Testnet)
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# === OPTIONAL ===
# CoinMarketCap API for gas reporting
CMC_API_KEY=your_cmc_api_key
